        "phpstan/extension-installer": ">=1.3.1",
        "phpstan/phpstan": ">=1.10.64",
        "phpstan/phpstan-deprecation-rules": ">=1.1.4",
        "phpstan/phpstan-strict-rules": ">=1.5.2"
    },
    "conflict": {
        "nikic/php-parser": "4.7.0"
    },
    "autoload": {
